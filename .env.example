ATR_PERIOD=14
QUOTE_ASSET=USDT
TIME_BASED_EXIT_HOURS=20
PROFIT_TRIGGER_PCT=0.05
LOCK_PERCENT_OF_TRIGGER=0.6
POSITION_CHECK_INTERVAL_SEC=30
STOP_LOSS_BALANCE_PCT=0.01
RUN_CANDIDATE_ON_START=false
